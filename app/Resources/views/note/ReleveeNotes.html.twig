{% extends 'base.html.twig' %}

{% block body %}
   <center><h1>Relevée des Notes</h1></center>
    <left>SUPTECH école d'ingénirié privée </left>
    <br><br>
   <h2>Nom d'étudiant: {{ nom }}</h2>
    <h2>Email : {{ email }}</h2>
    <br>

    <table class="table table-striped table-bordered table-hover dataTables-example">
        <thead>
        <tr>

            <th>Note controle continue</th>
            <th>Note DS</th>
            <th>Note d'examen</th>
            <th>Moyenne matière</th>
            <th>Matière</th>

        </tr>
        </thead>
        <tbody>
        {% for r in result %}
            <tr>

                <td>{{ r.note_cc }}</td>
                <td>{{ r.note_ds }}</td>
                <td>{{ r.note_examun }}</td>
                <td>{{ r.moyenne }}</td>
                <td>{{ r.nom_matier }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
<br> <br>


{% endblock %}
