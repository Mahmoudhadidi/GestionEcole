<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="annotation\Evenement" table="evenement">
    <id name="idEvenement" type="integer" column="id_evenement">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nomEvenement" type="string" column="nom_evenement" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="sujet" type="string" column="sujet" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dateEvenement" type="string" column="date_evenement" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="heureEvenement" type="string" column="heure_evenement" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
