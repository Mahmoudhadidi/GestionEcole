<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="annotation\Club" table="club">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idEtudiant" type="integer" column="id_etudiant" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="nom" type="string" column="nom" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="photoCouverture" type="string" column="photo_couverture" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="logo" type="string" column="logo" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="slogan" type="string" column="slogan" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="grandTitre" type="string" column="grand_titre" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="string" column="description" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="mail" type="string" column="mail" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="etat" type="string" column="etat" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
