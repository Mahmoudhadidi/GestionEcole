<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="annotation\Salle" table="salle">
    <id name="idSalle" type="integer" column="id_salle">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="numSalle" type="integer" column="num_salle" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="nomSalle" type="string" column="nom_salle" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="bloc" type="string" column="bloc" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
