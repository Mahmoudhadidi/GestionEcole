<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="annotation\Classe" table="classe">
    <indexes>
      <index name="num_classe" columns="num_classe"/>
      <index name="num_classe_2" columns="num_classe"/>
    </indexes>
    <id name="idClasse" type="integer" column="id_classe">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="numClasse" type="string" column="num_classe" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nbreEtudient" type="integer" column="nbre_etudient" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="specialite" type="string" column="specialite" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
