<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="annotation\Evaluation" table="evaluation">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idEtd" type="integer" column="id_etd" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="presence" type="string" column="presence" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="rendement" type="string" column="rendement" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="discipline" type="string" column="discipline" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nomMatiere" type="string" column="nom_matiere" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="idEnseignant" type="integer" column="id_enseignant" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
