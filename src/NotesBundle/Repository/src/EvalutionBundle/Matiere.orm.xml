<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="annotation\Matiere" table="matiere">
    <id name="idMatiere" type="integer" column="id_matiere">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nomMatier" type="string" column="nom_matier" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="coefficient" type="integer" column="coefficient" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="cdedit" type="integer" column="cdedit" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
