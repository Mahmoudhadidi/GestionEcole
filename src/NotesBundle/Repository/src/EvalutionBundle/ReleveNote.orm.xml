<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="annotation\ReleveNote" table="releve_note">
    <id name="idReleve" type="integer" column="id_releve">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="noteCcc" type="float" column="note_ccc" precision="10" scale="0" nullable="true">
      <options>
        <option name="default">NULL</option>
      </options>
    </field>
    <field name="noteDss" type="float" column="note_dss" precision="10" scale="0" nullable="true">
      <options>
        <option name="default">NULL</option>
      </options>
    </field>
    <field name="noteExamenn" type="float" column="note_examenn" precision="10" scale="0" nullable="false"/>
    <field name="moy" type="float" column="moy" precision="10" scale="0" nullable="false"/>
    <field name="nette" type="float" column="nette" precision="10" scale="0" nullable="false"/>
    <field name="nomMat" type="string" column="nom_mat" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="coeff" type="integer" column="coeff" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="name" type="string" column="name" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="secondName" type="string" column="second_name" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="idUserr" type="integer" column="id_userr" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
