<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="annotation\DemandeClub" table="demande_club">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idEtudiant" type="integer" column="id_etudiant" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="nomClub" type="string" column="nom_club" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="domaine" type="string" column="domaine" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="string" column="description" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="etat" type="string" column="etat" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
