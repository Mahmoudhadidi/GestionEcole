{% extends 'adminnote.html.twig' %}

{% block body %}
    <center><h1> Liste des Majeurs par matière </h1></center>
    <br> <br>

    <table class="table table-striped table-bordered table-hover dataTables-example">
        <thead>
        <tr>


            <th>Nom étudiant</th>
            <th>Matiére</th>
            <th> meilleur moyenne</th>


        </tr>
        </thead>
        <tbody>
        {% for r in result %}
            <tr>

                <td>{{ r.username }}</td>
                <td>{{ r.nom_matier }}</td>
                <td>{{ r.majeur }}</td>

            </tr>
        {% endfor %}
        </tbody>
    </table>
    <br> <br>

{% endblock %}